{
  "$id": "https://raw.githubusercontent.com/maximiliana-dev/mxa-json-schemas/master/realtime/grandpas/wakeup-settings.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WakeUpSettings",
  "type": "object",
  "required": ["accelerationThreshold"],
  "properties": {
    "accelerationThreshold": {
      "type": "number", "minimum": 0.0
    },
    "strategies": {
      "type": "object",
      "properties": {
        "PROXIMITY": {"type": "boolean"},
        "SHAKE": {"type": "boolean"}
      }
    }
  }
}