{
  "$id": "contact.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Contact",
  "type": "object",
  "required": ["displayName", "phoneNumber"],
  "properties": {
    "displayName": {"type": "string"},
    "phoneNumber": {"$ref": "https://raw.githubusercontent.com/maximiliana-dev/mxa-json-schemas/develop/realtime/shared/phone-number.schema.json"},
    "picture": {"$ref": "https://raw.githubusercontent.com/maximiliana-dev/mxa-json-schemas/develop/realtime/grandpa/picture.schema.json"},
    "position": {"type": "integer", "minimum": 0}
  }
}
